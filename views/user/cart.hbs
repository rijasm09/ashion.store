  <!-- Shop Cart Section Begin -->

  {{!-- <section class="shop-cart spad"> --}}
    {{#if getCartProducts}}
    <div class="container" style="margin-top: 2rem;">
      <div class="row">
        <div class="col-lg-12">
          <div class="shop__cart__table">
            <table>
              <thead>
                <tr>
                  <th>Product</th>
                  <th>Price</th>
                  <th>Quantity</th>
                  <th>Total</th>
                  <th></th>
                </tr>
              </thead>
              <tbody>
                {{#each getCartProducts}}
                <tr>
                  <td class="cart__product__item">
                    <img src="{{this.product.image.[0]}}" style="height: 90px;width:90px" alt="">
                    <div class="cart__product__item__title">
                      <h6>{{this.product.Name}}</h6>
                      {{!-- <div class="rating">
                        <i class="fa fa-star"></i>
                        <i class="fa fa-star"></i>
                        <i class="fa fa-star"></i>
                        <i class="fa fa-star"></i>
                        <i class="fa fa-star"></i>
                      </div> --}}
                    </div>
                  </td>
                  <td class="cart__price">₹ {{this.product.OfferPrice}} </td>

                  <td class="cart__quantity" style="">


                    <button class="btn btn-primary px-3" style="" id="minus{{this.product._id}}"
                      onclick=" changeQuantity('{{this._id}}' , '{{this.product._id}}','{{../userId}}','{{this.product.Stock}}',-1) ">
                      <i class="fas fa-minus"></i>
                    </button>

                    <div class="form-outline">
                      <input min="0" name="quantity" id="{{this.product._id}}" value="{{this.quantity}}" value="1"
                        type="text" class="form-control" style="width:48px" readonly />
                    </div>

                    <button class="btn btn-primary px-3" style="" id="plus{{this.product._id}}"
                      onclick=" changeQuantity('{{this._id}}' , '{{this.product._id}}','{{../userId}}','{{this.product.Stock}}',1)">
                      <i class="fas fa-plus"></i>
                    </button>


                  </td>

                  <td class="cart__total"><strong><span>₹ </span><span
                        id="{{this.product._id}}_0">{{this.total}}</span></strong></td>
                  <td class="cart__close"><span onclick="cartRemovePro('{{this._id}}' , '{{this.product._id}}')"
                      class="icon_close"></span></td>
                </tr>
                {{/each}}
              </tbody>
            </table>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-6 col-md-6 col-sm-6">
          <div class="cart__btn">
            <a href="/view-products">Continue Shopping</a>
          </div>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-6">
          <div class="cart__btn update__btn">
            <a href="/view-products"><span class="icon_loading"></span> Update cart</a>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-6">
          <div class="discount__content">
            {{!-- <h6>Discount codes</h6>
            <form action="#">
              <input type="text" placeholder="Enter your coupon code">
              <button type="submit" class="site-btn">Apply</button>
            </form> --}}
          </div>
        </div>
        <div class="col-lg-4 offset-lg-2">
          <div class="cart__total__procced">
            <h6>Cart total</h6>
            <ul>
              <li>Subtotal <span id="subtotal">₹ {{totalValue}}</span></li>
              <li>Shipping <span>₹ 0</span></li>
              <li>Total <span id="total">₹ {{totalValue}}</span></li>
            </ul>
            <a href="/place-order" class="primary-btn">Proceed to checkout</a>
          </div>
        </div>
      </div>
    </div>
    {{else}}
    <div class="container" style="margin-top: 2rem;">
      <h3 class="h-style">- Cart is Empty -</h3>
      <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
          <div class="cart__btn">
            <a href="/view-products">Continue Shopping</a>
          </div>
        </div>
      </div>
    </div>
    {{/if}}
    {{!--
  </section> --}}
