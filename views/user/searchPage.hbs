<body>
    <!-- Breadcrumb Begin -->
    <div class="breadcrumb-option">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="breadcrumb__links">
                        <a href="/"><i class="fa fa-home"></i> Home</a>
                        <span>Search</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Breadcrumb End -->

    <!-- Shop Section Begin -->
    {{!-- <section class="shop spad"> --}}
        <div class="container" style="margin-top: 2rem;">
            <div class="row">

                <div class="col-lg-12 col-md-12">
                    <div class="row">

                        {{#if response}}
                        {{#each response}}
                        <div class="col-lg-4 col-md-6">
                            <div class="product__item">
                                <div class="product__item__pic set-bg" data-setbg="{{this.image.[0]}}">
                                    <div class="label new">New</div>
                                    {{#if ../user}}
                                    {{#if this.Stock}}
                                    <ul class="product__hover">
                                        <li><a href="/productDetails/{{this._id}}"><span
                                                    class="arrow_expand"></span></a></li>

                                        <li><a onclick="addToWishlist('{{this._id}}')"><span
                                                    class="icon_heart_alt"></span></a></li>

                                        <li><a onclick="addToCart('{{this._id}}')"><span
                                                    class="icon_bag_alt"></span></a></li>
                                    </ul>
                                    {{else}}
                                    <div class="label stockout">out of stock</div>
                                    <ul class="product__hover">
                                        <li><a href="/productDetails/{{this._id}}"><span
                                                    class="arrow_expand"></span></a></li>

                                        <li><a onclick="addToWishlist('{{this._id}}')"><span
                                                    class="icon_heart_alt"></span></a></li>

                                        <li><a onclick=""><span class="icon_bag_alt"></span></a></li>
                                    </ul>
                                    {{/if}}
                                    {{else}}
                                    <ul class="product__hover">
                                        <li><a href="/productDetails/{{this._id}}"><span
                                                    class="arrow_expand"></span></a></li>

                                        <li><a href="/login"><span class="icon_heart_alt"></span></a></li>

                                        <li><a href="/login"><span class="icon_bag_alt"></span></a></li>
                                    </ul>

                                    {{/if}}
                                </div>
                                <div class="product__item__text">
                                    <h6><a href="#">{{this.Name}}</a></h6>
                                    {{!-- <div class="rating">
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                        <i class="fa fa-star"></i>
                                    </div> --}}
                                    <div class="product__price">â‚¹ {{this.OfferPrice}}</div>
                                </div>
                            </div>
                        </div>
                        {{/each}}
                        {{else}}
                        <div class="container">
                            <h2>Oops! Page not found.</h2>
                            <h1>
                                <ion-icon name="sad-outline"></ion-icon>
                            </h1>
                            <p>We can't find the result you're looking for.</p>
                            <a href="/">Go back home</a>
                        </div>
                        {{/if}}

                        <div class="col-lg-12 text-center">
                            <div class="pagination__option">
                                <a href="#">1</a>
                                <a href="#">2</a>
                                <a href="#">3</a>
                                <a href="#"><i class="fa fa-angle-right"></i></a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        {{!--
    </section> --}}
